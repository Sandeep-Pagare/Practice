class Parent
{
  void method1()
  {
    System.out.println("Parent class method1");
   }
}
class InheritancePractice extends Parent
{
  void method2()
  {
    System.out.println("Child class method2");
  }
  public static void main(String abc[])
  {
    InheritancePractice ip = new InheritancePractice();
    ip.method1();
    ip.method2();
   }
 }
    
